<Window x:Name="VentanaCodigoRegistro" x:Class="cliente.CodigoRegistro"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:cliente"
        mc:Ignorable="d"
        Title="CodigoRegistro" Height="450" Width="800">
    <Grid Background="#FF1A1D40">
        <Button x:Name="buttonCompletarRegistro" Content="{DynamicResource ResourceKey=ButtonSignUp}" HorizontalAlignment="Left" Margin="338,273,0,0" VerticalAlignment="Top" Width="152" Click="Button_Click" Height="36"/>
        <TextBox x:Name="textBoxCodigo" HorizontalAlignment="Left" Height="27" Margin="250,171,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="318" MaxLength="3" TextChanged="TextBoxCodigo_TextChanged" PreviewTextInput="TextBoxCodigo_PreviewTextInput" />
        <TextBlock HorizontalAlignment="Left" Margin="115,70,0,0" TextWrapping="Wrap" Text="{DynamicResource ResourceKey=TittleCodeLogin}" VerticalAlignment="Top" FontSize="24" Foreground="#FF0D8585" FontFamily="Segoe UI Semibold" Height="55" Width="660"/>

    </Grid>
</Window>
