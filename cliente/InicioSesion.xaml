<Window x:Name="VentanaInicioDeSesion" x:Class="cliente.InicioSesion"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:cliente"
        mc:Ignorable="d"
        Title="InicioSesion" Height="450" Width="800">
    <Grid Background="#FF1A1D40">
        <TextBox x:Name="textBoxNombreDeUsuario" HorizontalAlignment="Left" Height="25" Margin="194,139,0,0" TextWrapping="Wrap" Text="Nombre de usuario" VerticalAlignment="Top" Width="344" MaxLength="20" PreviewTextInput="TextBoxNombreDeUsuario_PreviewTextInput" TextChanged="TextBoxNombreDeUsuario_TextChanged"/>
        <Button x:Name="buttonIniciarSesion" Content="Iniciar sesión" HorizontalAlignment="Left" Margin="288,296,0,0" VerticalAlignment="Top" Width="111" Height="41" Click="Button_Click"/>
        <Button x:Name="buttonRegresar" Content="regresar" HorizontalAlignment="Left" Margin="30,33,0,0" VerticalAlignment="Top" Width="88" Height="31" Click="ButtonRegresar_Click"/>
        <PasswordBox x:Name="textBoxContrasena" HorizontalAlignment="Left" Margin="194,215,0,0" VerticalAlignment="Top" Width="344" Height="25" MaxLength="18" PreviewTextInput="TextBoxContrasena_PreviewTextInput" DataContextChanged="TextBoxContrasena_DataContextChanged" />

    </Grid>
</Window>
